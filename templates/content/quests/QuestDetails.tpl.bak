                                        {$tab_count = 0}
                                    {else}
                                        {$tab_count = $tab_count + 1}
                                    {/if}
                                    
                                    {$last_link = $link}
                                    
                                    {$dialog = $quest->{$link['d']}}
                                    {$message = $dialog[$link['m']]['message']}
                                    {$option = $dialog[$link['m']]['options'][$link['o']]['text']}
